cantidad = 0
cantidad2 = 0
cantidad3 = 0


for parentesis in niveles:

    if "(" in parentesis: 
        cantidad+=1

    elif ")" in parentesis: 
        cantidad-=1

print(cantidad)

for parentessis in niveles:

    if cantidad2 < 0:
        break
    elif "(" in parentessis:
        cantidad2+=1
        cantidad3+=1
    elif ")" in parentessis:
        cantidad2-=1
        cantidad3+=1


print (cantidad3)
        

